{
    "ecmaFeatures": {
        "blockBindings": true,
        "forOf": true,
        "jsx": true,
        "modules": true
    },

    "rules": {
        "semi": 2,
        "new-cap": false,
        "comma-dangle": "never"
    },

    "env": {
      "browser": true,
      "node": true
    },

    "parser": "babel-eslint",
    "plugins": [
      "react"
    ]
}
